<section class="products" >
    <h1>Pizzeria Coderhouse</h1>
    <div id="products-box" class="products__container">
        {{#each products}}
            <div class="product__card">
                <img src={{this.thumbnail}} alt={{this.title}}>
                <h1>{{this.title}}</h1>
                <div class="product__card__information">
                    <p>${{this.price}}</p>
                    <p>Stock:{{this.stock}}</p>
                </div>
                <button onclick="alert('Es solo estetico')">Agregar</button>
            </div>
        {{/each}}
    </div>
</section>

<script src="/socket.io/socket.io.js"></script>
<script src="/index.js"></script>

